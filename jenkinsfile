pipeline{
    agent any
    stages{
        stage('Git Checkout'){
            steps{
                git credentialsId: 'github', url: 'https://github.com/Moni3099/my-app'
                
            }
        }
        stage('Mvn sh cmd')
           steps{
               def mvnHome = tool name: 'maven', type: 'maven'
               def mvnCMD = "${mvnHome}/bin/mvn"
               sh "${mvnCMD} clean package"
               }
           }
      }
 }
 
