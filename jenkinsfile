pipeline{
    agent any
    stages{
        stage('Git Checkout'){
            steps{
                git credentialsId: 'github', url: 'https://github.com/Moni3099/my-app'
                
            }
        }
        stage('sh Cmd'){
           steps{
               sh label: '', script: 'mvn  clean  package'
               }
           }
      }
 }
 
